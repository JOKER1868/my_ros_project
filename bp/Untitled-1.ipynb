    "import torch\n",
    "\n",
    "# 创建张量\n",
    "x = torch.tensor([[1., 2.], [3., 4.]])\n",
    "y = torch.tensor([[5., 6.], [7., 8.]])\n",
    "\n",
    "# 张量加法\n",
    "z = x + y\n",
    "print(\"z =\", z)\n",
    "\n",
    "# 矩阵乘法\n",
    "w = torch.matmul(x, y)\n",
    "print(\"w =\", w)\n"
   ]
  }
